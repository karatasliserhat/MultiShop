@model List<ResultFeatureDto>
<div class="container-fluid pt-5">
    <div class="row px-xl-5 pb-3">
        @if (Model is not null)
        {
            foreach (var item in Model)
            {
                <div class="col-lg-3 col-md-6 col-sm-12 pb-1">
                    <div class="d-flex align-items-center bg-light mb-4" style="padding: 30px;">
                        <h1 class="fa fa-@item.IconUrl text-primary m-0 mr-3"></h1>
                        <h5 class="font-weight-semi-bold m-0">@item.Title</h5>
                    </div>
                </div>
            }
        }
        else
        {
            <div class="col-lg-3 col-md-6 col-sm-12 pb-1">
                <div class="d-flex align-items-center bg-light mb-4" style="padding: 30px;">
                    <h1 class="fa fa-info text-primary m-0 mr-3"></h1>
                    <h5 class="font-weight-semi-bold m-0">Özel Alan Eklemesi Yapnız</h5>
                </div>
            </div>
        }
    </div>
</div>